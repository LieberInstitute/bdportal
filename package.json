  "dependencies": {
    "axios": "^0.26.0",
    "bootstrap": "^4.6.1",
    "datatables.net-bs4": "^1.11.4",
    "datatables.net-buttons-bs4": "^2.0.1",
    "datatables.net-fixedcolumns-bs4": "^4.0.1",
